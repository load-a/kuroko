MOVE 111 A
SAVE A $10
MOVE 222 B
SAVE B $11

PUSH A
PUSH B

POP A
POP B

move 0x53 j
move 0x74 i
move 0x61 l
move 0x63 h
move 0x6b c
move 0x21 b
move 0x00 a
DUMP

MOVE 248 A
MOVE 255 i
MOVE 12 j

CALL :WRITE_TO_MEMORY
RSTR

CALL :CLEAR_REGISTERS
HALT

WRITE_TO_MEMORY:
	MOVE @I @J
	DEC I
	INC J
	JGT :WRITE_TO_MEMORY I
	RTRN
CLEAR_REGISTERS:
	move 0x43 a
	move 0x6c b
	move 0x65 c
	move 0x61 h
	move 0x72 l
	move 0x65 i
	move 0x64 j
	RTRN

